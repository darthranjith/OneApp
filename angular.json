{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "oneapp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/oneapp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/css/fonts/fontello/css/fontello.css",
              "src/assets/css/768.css",
              "src/assets/css/align.css",
              "src/assets/css/layout.css",
              "src/assets/css/main.css",
              "src/assets/css/normalize.css",
              "src/assets/css/demo4.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "oneapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "oneapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "oneapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/css/768.css",
              "src/assets/css/align.css",
              "src/assets/css/layout.css",
              "src/assets/css/main.css",
              "src/assets/css/demo5.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "oneapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "oneapp:serve:production"
            }
          }
        }
      }
    },
    "spicinemas-scrapper": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/spicinemas-scrapper",
      "sourceRoot": "projects/spicinemas-scrapper/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/spicinemas-scrapper",
            "index": "projects/spicinemas-scrapper/src/index.html",
            "main": "projects/spicinemas-scrapper/src/main.ts",
            "polyfills": "projects/spicinemas-scrapper/src/polyfills.ts",
            "tsConfig": "projects/spicinemas-scrapper/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/spicinemas-scrapper/src/favicon.ico",
              "projects/spicinemas-scrapper/src/assets"
            ],
            "styles": [
              "projects/spicinemas-scrapper/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/spicinemas-scrapper/src/environments/environment.ts",
                  "with": "projects/spicinemas-scrapper/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "spicinemas-scrapper:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "spicinemas-scrapper:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "spicinemas-scrapper:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/spicinemas-scrapper/src/test.ts",
            "polyfills": "projects/spicinemas-scrapper/src/polyfills.ts",
            "tsConfig": "projects/spicinemas-scrapper/tsconfig.spec.json",
            "karmaConfig": "projects/spicinemas-scrapper/karma.conf.js",
            "assets": [
              "projects/spicinemas-scrapper/src/favicon.ico",
              "projects/spicinemas-scrapper/src/assets"
            ],
            "styles": [
              "projects/spicinemas-scrapper/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/spicinemas-scrapper/tsconfig.app.json",
              "projects/spicinemas-scrapper/tsconfig.spec.json",
              "projects/spicinemas-scrapper/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/spicinemas-scrapper/e2e/protractor.conf.js",
            "devServerTarget": "spicinemas-scrapper:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "spicinemas-scrapper:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "oneapp",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
